    "octonode": "^0.7.4",
    "parse-link-header": "^0.4.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "react-timeago": "^2.2.1",
    "request-promise": "^2.0.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
