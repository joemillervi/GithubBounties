{
  "name": "dapper",
  "version": "1.0.0",
  "description": "'awesomeness'",
  "engines": {
    "node": "4.2.4"
  },
  "main": "client/app.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node server/server.js",
    "testclient": "mocha client/spec/ClientTest.js",
    "testserver": "mocha server/spec/ServerTest.js",
    "testdata": "mocha server/spec/DataTest.js",
    "build": "webpack",
    "dev": "webpack-dev-server --devtool source-map --progress --colors --hot --content-base client",
    "prodbuild": "NODE_ENV=production webpack -p",
    "startForever": "NODE_ENV=production forever start --spinSleepTime 10000 server/server.js",
    "restartForever": "NODE_ENV=production forever restart server/server.js"
  },
